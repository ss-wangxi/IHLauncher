<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.btime.launcher" >

    <!-- 流量监控2x3 -->
    <widgetview
        launcher:container="-100"
        launcher:screen="0"
        launcher:spanX="2"
        launcher:spanY="3"
        launcher:widgetViewType="1005"
        launcher:x="0"
        launcher:y="0" />
    
    <!-- 设置2x3 -->
    <widgetview
        launcher:container="-100"
        launcher:screen="0"
        launcher:spanX="2"
        launcher:spanY="3"
        launcher:widgetViewType="1008"
        launcher:x="2"
        launcher:y="0" />

    <!-- 日历1x1 -->
    <widgetview
        launcher:container="-100"
        launcher:screen="0"
        launcher:spanX="1"
        launcher:spanY="1"
        launcher:widgetViewType="102"
        launcher:x="4"
        launcher:y="0" />

    <!-- 时钟1x1 -->
    <widgetview
        launcher:container="-100"
        launcher:screen="0"
        launcher:spanX="1"
        launcher:spanY="1"
        launcher:widgetViewType="101"
        launcher:x="4"
        launcher:y="1" />

</favorites>